name: guptik
description: "A new Flutter project."
publish_to: 'none'
version: 0.1.0

environment:
  sdk: ">=3.10.7 <4.0.0"

dependencies:
  flutter:
    sdk: flutter
  cupertino_icons: ^1.0.2
  
  # Database & Backend
  supabase_flutter: ^2.10.2
  
  # QR Code Scanning
  mobile_scanner: ^7.1.4
  
  # Permissions
  permission_handler: ^12.0.1
  
  # Media Picking
  image_picker: ^1.0.4
  
  # File Picking
  file_picker: ^5.2.7
  
  # Network
  http: ^1.1.0
  
  # Deeplinking & Routing
  app_links: ^6.3.2
  go_router: ^17.0.1
  
  # Security
  crypto: ^3.0.3
  
  # Icons
  font_awesome_flutter: ^10.5.0
  
  # HTTP Parsing
  http_parser: ^4.0.2
  
  # Audio
  just_audio: ^0.10.2
  record: 6.2.0
  
  # Video Player
  video_player: ^2.8.3
  
  # Local Storage
  shared_preferences: ^2.2.2
  
  # Date/Time Formatting
  intl: ^0.19.0
  
  # File Handling
  path_provider: ^2.1.3
  path: ^1.8.3
  
  # Image Caching
  cached_network_image: ^3.3.0
  
  # State Management
  provider: ^6.1.2
  
  # Emoji Picker
  emoji_picker_flutter: ^2.0.2
  
  # Camera (COMPATIBLE VERSION)
  camera: ^0.10.5+1  # Changed to compatible version

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^3.0.1
  flutter_launcher_icons: ^0.14.4

flutter:
  uses-material-design: true
  
  assets:
    - assets/images/
    - assets/icons/

flutter_icons:
  android: true
  ios: true
  image_path: "assets/images/MetaFly.png"